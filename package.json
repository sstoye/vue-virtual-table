{
  "name": "vue-virtual-table",
  "version": "0.2.15",
  "description": "vue table component with virtual dom",
  "author": "https://github.com/waningflow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sstoye/vue-virtual-table.git"
  },
  "keywords": [
    "vue",
    "virtual",
    "component",
    "table",
    "smooth",
    "easy"
  ],
  "main": "dist/vue-virtual-table.umd.js",
  "module": "dist/vue-virtual-table.esm.js",
  "unpkg": "dist/vue-virtual-table.min.js",
  "browser": {
    "./sfc": "src/vue-virtual-table.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "v-tooltip": "2.0.2",
    "vue-observe-visibility": "^0.4.3",
    "vue-resize": "^0.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.11.0",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  }
}
